      },
      "cell_type": "code",
      "source": [
        "rp_mat = rp.as_matrix()"
      ],
      "execution_count": 0,
      "outputs": []
    },
    {
      "metadata": {
        "id": "RZHZBYNzLgO7",
        "colab_type": "code",
        "colab": {}
      },
      "cell_type": "code",
      "source": [
        "from scipy.spatial.distance import cosine\n",
        "m,n = rp.shape\n",
        "mat_users = np.zeros((m, m))"
      ],
      "execution_count": 0,
      "outputs": []
    },
    {
      "metadata": {
        "id": "78-33SvGL6-7",
        "colab_type": "code",
        "colab": {}
      },
      "cell_type": "code",
      "source": [
        "for i in range(m):\n",
        "  for j in range(m):\n",
        "    if i != j:\n",
        "      mat_users[i][j] = (1 - cosine(rp_mat[i,:], rp_mat[j,:]))\n",
        "    else:\n",
        "      mat_users[i][j] = 0\n",
        "      \n",
        "pd_users = pd.DataFrame(mat_users, index = rp.index, columns = rp.index)\n",
        "    "
      ],
      "execution_count": 0,
      "outputs": []
    },
    {
      "metadata": {
        "id": "IyeOjzQLTZlk",
        "colab_type": "code",
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 153
        },
        "outputId": "3c676a1a-4749-49a8-8456-30fe4f836af6"
      },
      "cell_type": "code",
      "source": [
        "def topn_simusers(uid = 16, n=5):\n",
        "  users = pd_users.loc[uid, :].sort_values(ascending=False)\n",
        "  print(users.shape)\n",
        "  topn_users = users.iloc[:n,]\n",
        "  topn_users = topn_users.rename('score')\n",
        "  return pd.DataFrame(topn_users)\n",
        "\n",
        "print(topn_simusers())"
      ],
      "execution_count": 21,
      "outputs": [
        {
          "output_type": "stream",
          "text": [
