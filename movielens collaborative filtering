        "# Movie similarity\n",
        "import time\n",
        "start_time = time.time()\n",
        "mat_movies = np.zeros((n,n))\n",
        "\n",
        "for i in range(n):\n",
        "  for j in range(n):\n",
        "    if i!=j:\n",
        "      mat_movies[i, j] = (1 -cosine(rp_mat[:, i], rp_mat[:, j]))\n",
        "    else:\n",
        "      mat_movies[i, j]=0\n",
        "\n",
